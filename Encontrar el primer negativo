//El programa se detiene cuando se ingresa un numero negativo

#include <stdio.h>
#include <math.h>

int main (){
int d;

printf("Imprimir el primer numero negativo que se ingrese\n\n");

while (0<=d){
printf("Ingrese un numero: ");
scanf("%d", &d); 

}
printf("El numero negativo es: %d", d);

return 0;
}
